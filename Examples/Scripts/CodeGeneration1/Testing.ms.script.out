 STRING FUNCTION (+)?
  %SUMMARY %SUM
   'Если aSuffix не пустой, то возвращает сумму aPrefix и aSuffix, иначе возвращает пустую строку'
  ; // %SUMMARY %SUM
  
  ^ IN _aSuffix
  STRING IN aPrefix
  IMMEDIATE VOID OPERATOR aSuffix
   '_aSuffix'
   MangledNamePusher
  ; // IMMEDIATE VOID OPERATOR aSuffix
  
  STRING VAR l_Suffix
  Result :=
  ''
  (
   _aSuffix |^
  )
  >>>
  l_Suffix
  if
   (
    l_Suffix
    <>
    ''
   )
  then
   (
    aPrefix
    l_Suffix
    Cat
    >>>
    Result
   )
 ; // STRING FUNCTION (+)?
 
 STRING CONST cTests
  'Tests:'
 IMMEDIATE VOID MACRO RunTests
  ^L IN aName
  STRING VAR l_Name
  cTests
  aName
  |N
  Cat
  >>>
  l_Name
  [
   '@'
   l_Name
   'MembersIterator'
   '==>'
   '@ Print DO'
  ]
  Ctx:Parser:PushArray
 ; // IMMEDIATE VOID MACRO RunTests
 
 IMMEDIATE VOID MACRO Tests
  ^L IN aName
  [
   'VOID axiom:operator'
   cTests
   aName
   |N
   Cat
  ]
  Ctx:Parser:PushArray
 ; // IMMEDIATE VOID MACRO Tests
 
 VOID OPERATOR Tests:(+)?
  VOID Test T1
   ''
   (+)?
   'B'
   .
  ; // VOID Test T1
  
  VOID Test T2
   'A'
   (+)?
   'B'
   .
  ; // VOID Test T2
  
  VOID Test T3
   'A'
   (+)?
   ''
   .
  ; // VOID Test T3
  
 ; // VOID OPERATOR Tests:(+)?
 
