VOID Test TestingTest
 STRING FUNCTION (+)?
  %SUMMARY %SUM
   'Если aSuffix не пустой, то возвращает сумму aPrefix и aSuffix, иначе возвращает пустую строку'
  ; // %SUMMARY %SUM
  
  ^ IN _aSuffix
  STRING IN aPrefix
  IMMEDIATE VOID OPERATOR aSuffix
   '_aSuffix'
   MangledNamePusher
  ; // IMMEDIATE VOID OPERATOR aSuffix
  
  STRING VAR l_Suffix
  Result :=
  ''
  (
   _aSuffix |^
  )
  >>>
  l_Suffix
  if
   (
    l_Suffix
    <>
    ''
   )
  then
   (
    aPrefix
    l_Suffix
    Cat
    >>>
    Result
   )
 ; // STRING FUNCTION (+)?
 
 IMMEDIATE VOID MACRO Tests
  ^L IN aName
  [
   'VOID axiom:operator'
   'Tests:'
   aName
   |N
   Cat
  ]
  Ctx:Parser:PushArray
 ; // IMMEDIATE VOID MACRO Tests
 
 VOID OPERATOR Tests:(+)?
  %%Tests %Tests
   VOID Test T1
    ''
    (+)?
    'B'
    .
   ; // VOID Test T1
   
   VOID Test T2
    'A'
    (+)?
    'B'
    .
   ; // VOID Test T2
   
   VOID Test T3
    'A'
    (+)?
    ''
    .
   ; // VOID Test T3
   
  ; // %%Tests %Tests
  
 ; // VOID OPERATOR Tests:(+)?
 
 TestingTest
 DumpElement
; // VOID Test TestingTest

