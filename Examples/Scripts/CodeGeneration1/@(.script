// @(.script

USES
 io.ms.dict
 macro.ms.dict
;

MACRO @(
 INTERFACE VAR l_Parser 
 Ctx:Parser >>> l_Parser
 ARRAY VAR l_A
 l_A := [ ]

 WHILE true
 BEGIN
  STRING VAR l_Token
  l_Parser pop:Parser:TokenLongString >>> l_Token
  if ( l_Token = ')' ) then
   ( 
    BREAK
   )
  l_Token Print
  l_Parser pop:Parser:NextToken
 END
; // @(

